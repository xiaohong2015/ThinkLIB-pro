{
  "type": "library",
  "name": "zoujingli/think-library",
  "license": "MIT",
  "homepage": "http://framework.thinkadmin.top",
  "description": "ThinkPHP v6.0 Basic Development Library",
  "authors": [
    {
      "name": "Anyon",
      "email": "zoujingli@qq.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "topthink/framework": "6.0.x-dev",
    "topthink/think-trace": "dev-master"
  },
  "autoload": {
    "files": [
      "src/common.php"
    ],
    "psr-4": {
      "app\\": "app",
      "think\\admin\\": "src"
    }
  },
  "extra": {
    "think": {
      "services": [
        "think\\admin\\ThinkLibrary"
      ]
    }
  },
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "dist"
  },
  "scripts": {
    "post-autoload-dump": [
      "@php think service:discover",
      "@php think vendor:publish"
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer"
    }
  }
}
